#include <stdio.h>
#include "holberton.h"

/**
 * print_most_numbers - Entry point
 * Description: use _putchar to print 0 to 9 to screen (no 2 & 4)
 *
 * Return: nothing
 */
void print_most_numbers(void)
{
	char i = '0';

	while (i <= '9')
	{
		if (i != '2' || i != '4')
			_putchar(i);
		i++;
	}
	_putchar('\n');
}
